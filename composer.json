{
  "name": "anwalt/image-modifier",
  "type": "project",
  "license": "MIT",
  "description": "Modifies images with different modifiers and returns the resulted image",
  "require-dev": {
    "pestphp/pest": "^v2.2",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "require": {
    "php": "^8.1",
    "symfony/http-foundation": "^v6.2",
    "symfony/routing": "^v6.2",
    "ext-imagick": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "optimize-autoloader": true
  }
}
